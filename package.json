{
  "name": "contumst-ui-v2",
  "private": false,
  "version": "1.0.9",
  "type": "module",
  "main": "./dist/vni-ui.umd.js",
  "module": "./dist/vni-ui.es.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/vni-ui.es.js",
      "require": "./dist/vni-ui.umd.js"
    },
    "./dist/contumst-ui.css": {
      "default": "./dist/contumst-ui-v2.css"
    },
    "./dist/contumst-ui-v2.css": {
      "default": "./dist/contumst-ui-v2.css"
    },
    "./style.css": {
      "default": "./dist/contumst-ui-v2.css"
    },
    "./dist/*.css": {
      "default": "./dist/*.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.4"
  }
}
